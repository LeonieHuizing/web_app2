package com.klm.api.operation.services;

import java.util.ArrayList;
import java.util.List;


public class OperationServices {

	public List<OperationServices> getAllRosterData(){
		OperationServices employee1 = new OperationServices(1L, "111", "Alfa");
		OperationServices employee2 = new OperationServices(2L, "222", "Beta");
		
		List<OperationServices> rosterDataList = new ArrayList<>();
		 	rosterDataList.add(employee1);
		 	rosterDataList.add(employee2);
	return rosterDataList;
	}
}
	
	
